if (TMB:::isNullPointer(oldfit$obj$env$ADFun$ptr)) {
obj <- oldfit$obj
oldfit$obj <- TMB::MakeADFun(obj$env$data,
obj$env$parameters,
map = obj$env$map,
random = obj$env$random,
silent = obj$env$silent,
DLL = "glmmTMB")
oldfit$obj$env$last.par.best <- obj$env$last.par.best
}
oldfit
}
